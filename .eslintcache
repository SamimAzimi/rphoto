, setPerson] = useState({\n    fname: \"\",\n    email: \"\",\n    subject: \"\",\n    message: \"\",\n  });\n  const handleChange = (e) => {\n    const name = e.target.name;\n    const value = e.target.value;\n    setPerson({ ...person, [name]: value });\n  };\n  const handlesubmit = (e) => {\n    e.preventDefault();\n    if (person.fname && person.email && person.subject && person.message) {\n      if (ValidateEmail(person.email)) {\n        const contactmessageRef = fire.database().ref(\"messages\");\n        contactmessageRef.push(person, (error) => {\n          if (error) {\n            console.log(error);\n            toast.info(\"kindly submit the form again\");\n          } else {\n            toast.info(\"Your Message has been sent to rphoto successfully\");\n            toast.info(\"Thank You for your message , we will contact you shortly\");\n            setPerson({ fname: \"\", email: \"\", subject: \"\", message: \"\" });\n          }\n        });\n      }\n    } else {\n      toast.info(\"Please Fill The Form\");\n    }\n  };\n\n  return (\n    <>\n      <form onSubmit={handlesubmit} className=\"contactform\">\n        <label htmlFor=\"name\">\n          <FontAwesomeIcon className=\"formIcons\" icon={faUser} />\n          <input\n            value={person.fname}\n            onChange={handleChange}\n            name=\"fname\"\n            id=\"fname\"\n            type=\"text\"\n            placeholder=\"Name\"\n            />\n            </label>\n\n        <label htmlFor=\"email\">\n          <FontAwesomeIcon className=\"formIcons\" icon={faEnvelope} />\n          <input\n            value={person.email}\n            name=\"email\"\n            onChange={handleChange}\n            id=\"email\"\n            required\n            type=\"text\"\n            placeholder=\"Email Address:\"\n          />\n        </label>\n\n        <label htmlFor=\"Subject\">\n          <FontAwesomeIcon className=\"formIcons\" icon={faTag} />\n          <input\n            value={person.subject}\n            onChange={handleChange}\n            name=\"subject\"\n            id=\"subject\"\n            type=\"text\"\n            placeholder=\"Subject\"\n          />\n        </label>\n      \n        <label htmlFor=\"Message\">\n          <FontAwesomeIcon className=\"formIcons\" icon={faComment} />\n          < textarea rows=\"4\" cols=\"70\"\n            type=\"text\"\n            value={person.message}\n            onChange={handleChange}\n            name=\"message\"\n            id=\"message\"\n            placeholder=\"Message\"\n          />\n        </label>\n\n        <button type=\"submit\" className=\"sendContactFormButton\">\n          <FontAwesomeIcon icon={faPaperPlane} /> Send\n        </button>\n      </form>\n      <ToastContainer\n        position=\"top-right\"\n        autoClose={5000}\n        hideProgressBar={false}\n        newestOnTop={false}\n        closeOnClick\n        rtl={false}\n        pauseOnFocusLoss\n        draggable\n        pauseOnHover\n      />\n    </>\n  );\n}\n\nexport default Contact;\n",["94","95"],"/home/subhan/rphoto/src/components/Package.js",[],"/home/subhan/rphoto/src/index.js",[],"/home/subhan/rphoto/src/components/AdminLogin.js",[],["96","97"],"/home/subhan/rphoto/src/firebase.config.js",[],"/home/subhan/rphoto/src/components/AdminPanel.js",[],"/home/subhan/rphoto/src/components/HomeSlider.js",[],["98","99"],"/home/subhan/rphoto/src/data/slidersData.js",[],"/home/subhan/rphoto/src/components/Gallary.js",[],"/home/subhan/rphoto/src/components/PrivateRoute.js",[],["100","101"],"/home/subhan/rphoto/src/components/SocialMedia.js",["102","103","104","105","106","107","108","109","110","111"],"import React from \"react\";\nimport \"../style/socialMedia.css\";\nexport default function SocialMedia() {\n  return (\n    <div class=\"container\">\n      <ul class=\"social\">\n        <li class=\"facebook\">\n          <a href=\"#\" class=\"entypo-facebook\"></a>\n        </li>\n        <li clas